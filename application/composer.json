{
    "name": "arens-myzyri/data-transformation-and-serialization",
    "type": "symfony-bundle",
    "version": "v1.0.2",
    "require": {
        "php": ">=8.1",
        "symfony/serializer": "^6.2.0",
        "symfony/runtime": "^6.2.0",
        "symfony/framework-bundle": "^6.2.0",
        "symfony/console": "^v6.2.0",
        "symfony/property-access": "^6.2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/yaml": "^6.2.0",
        "symfony/phpunit-bridge": "^6.2.0",
        "phpunit/phpunit": "^11.3"

    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
